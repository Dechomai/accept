    "react-dom": "^16.4.0",
    "react-dropzone": "^4.2.11",
    "react-redux": "^5.0.7",
    "react-router": "3",
    "react-router-dom": "^4.2.2",
    "react-toastify": "^4.1.0",
    "reactstrap": "^6.1.0",
    "recompact": "^3.3.0",
