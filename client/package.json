  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rimraf ./build/*",
    "copy:config": "ncp ../config/contracts src/config/contracts",
    "test": "jest -c jest.config.js --verbose",
    "test:watch": "yarn test --watchAll",
    "start": "npm-run-all copy:config start:webpack",
    "start:webpack": "cross-env NODE_ENV=development webpack-dev-server --mode=development",
    "build": "npm-run-all clean copy:config build:*",
    "build:code": "cross-env NODE_ENV=production webpack --mode=production",
    "build:assets": "ncp src/assets build/assets",
    "serve": "http-server ./build -p 7002 -o",
    "cognito-ui": "http-server ./cognito-ui -p 7003 -o"
  },
